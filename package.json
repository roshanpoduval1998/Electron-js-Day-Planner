{
  "name": "DayPlanner",
  "version": "1.0.0",
  "description": "Day Planner Javascript app\\",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=assets/icons/mac/icon.icns --prune=true --out=release-builds",
    "package-win": "electron-packager . electron--app --overwrite --asar=true --platform=win32 --arch=ia32 --icon=assets/icons/win/icon.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"Day Planner\"",
    "package-linux": "electron-packager . electro-app --overwrite --asar=true --platform=linux --arch=x64 --icon=assets/icons/png/icon.png --prune=true --out=release-builds"
  },
  "author": "Roshan poduval",
  "license": "MIT",
  "dependencies": {
    "electron": "^4.1.4"
  },
  "devDependencies": {
    "electron-packager": "^13.1.1"
  }
}
